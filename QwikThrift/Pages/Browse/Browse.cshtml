@page
@model QwikThrift.Pages.Browse.BrowseModel
@{
    ViewData["Title"] = "Browse Listings";
}

<h1>Browse Listings</h1>

<div class="row">
    <div class="col-md-6 cust-view">
        <form method="get">
            @Html.AntiForgeryToken()
            <div class="form-group">
                <label>Image:</label>
            </div>
            <div class="form-group">
                <label>Title:</label>
            </div>
            <div class="form-group">
                <label>Price:</label>
            </div>
            <div class="form-group">
                <label>Category:</label>
            </div>
            <div class="form-group">
                <label>Decription:</label>
            </div>
        </form>
    </div>
    <div class="col-md-4" style="text-align:center;padding:10px;">
        <button class="btn btn-primary">
            <a class="text-dark" asp-area="" asp-page="/Browse/Index">Contact Seller</a>
        </button>
    </div>
</div>